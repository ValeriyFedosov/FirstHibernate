<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ua.ustiuhova.entity.PhonesEntity" table="phones" schema="tryhibernate">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="personId">
            <column name="person_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="tphone">
            <column name="tphone" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>