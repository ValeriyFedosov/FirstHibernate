<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ua.ustiuhova.entity.PersonEntity" table="person" schema="tryhibernate">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="fio">
            <column name="fio" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>